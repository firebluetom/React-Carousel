{
  "name": "ReactCarousel",
  "version": "1.0.0",
  "description": "An image carousel built in ReactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --progress --profile --colors",
    "test": "jest",
    "lint": "eslint --ext='.js,.jsx' *.js src/**/*.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firebluetom/React-Carousel.git"
  },
  "keywords": [
    "react",
    "js",
    "jsx",
    "Carousel"
  ],
  "author": "firebluetom",
  "license": "MIT",
  "jest": {
    "rootDir": "./",
    "testPathDirs": [
      "./tests"
    ],
    "testRegex": ".js",
    "automock": true,
    "unmockedModulePathPatterns": [
      "node_modules"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest",
    "bail": false,
    "timers": "fake"
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-jest": "^15.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "css-loader": "0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "0.9.0",
    "jest": "^15.1.1",
    "node-sass": "^3.9.2",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "1.3.0",
    "sass-loader": "^4.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.13.2"
  }
}
